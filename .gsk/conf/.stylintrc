{
    "blocks"                : false,
    "brackets"              : "always",
    "colons"                : "always",
    "colors"                : "always",
    "commaSpace"            : "always",
    "commentSpace"          : "always",
    "cssLiteral"            : "never",
    "depthLimit"            : 5,
    "duplicates"            : true,
    "efficient"             : "always",
    "extendPref"            : "@extend",
    "globalDupe"            : true,
    "indentPref"            : 2,
    "leadingZero"           : false,
    "maxErrors"             : false,
    "maxWarnings"           : false,
    "mixed"                 : true,
    "namingConvention"      : "BEM",
    "namingConventionStrict": true,
    "none"                  : false,
    "noImportant"           : true,
    "parenSpace"            : false,
    "placeholders"          : false,
    "prefixVarsWithDollar"  : "always",
    "quotePref"             : "single",
    "semicolons"            : "always",
    "sortOrder"             : false,
    "stackedProperties"     : "never",
    "trailingWhitespace"    : "never",
    "universal"             : false,
    "valid"                 : true,
    "zeroUnits"             : "never",
    "zIndexNormalize"       : false,

    "mixins": []
}
